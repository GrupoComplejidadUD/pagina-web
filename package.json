    "hexo-renderer-pug": "^1.0.0",
    "hexo-server": "^1.0.0"
  },
  "devDependencies": {
    "bootstrap": "^4.5.2",
    "hexo-browsersync": "^0.3.0",
    "node-sass": "^4.14.1"
