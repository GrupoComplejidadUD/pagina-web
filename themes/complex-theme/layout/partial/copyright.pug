#suscribeModal.modal.fade(tabindex='-1', role='dialog', aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5#exampleModalLabel.modal-title.modal-title-footer Complex UD
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') ×
      .modal-body
        iframe.suscribe-form(src='https://docs.google.com/forms/d/e/1FAIpQLSegaAp6tL8oIdA360l6Jb1hpAuYrhzCaR5mQvrPz4RM4wQY-A/viewform?embedded=true') Cargando…
      .modal-footer
        button.btn.btn-secondary(type='button', data-dismiss='modal') Cerrar
  
footer
  .container-fluid.bg-dark
    hr
    .row.footer-main-container
      .element-footer
        p SIGUENOS EN
        hr
        .row
          .col.text-center
            a(href='https://www.facebook.com/Grupo-de-Complejidad-de-la-Universidad-Distrital-567936853231644/', target='_blank')
              img.rounded.mx-auto.d-block.social-icon-footer(src=url_for(theme.socials.facebook), alt='Facebook')
          .col.text-center
            a(href='https://twitter.com/complejidadud', target='_blank')
              img.rounded.mx-auto.d-block.social-icon-footer(src=url_for(theme.socials.twitter), alt='Twitter')
          .col.text-center
            a(href='https://www.youtube.com/channel/UCwMNplUJd2iBbNArsSeRsIQ', target='_blank')
              img.rounded.mx-auto.d-block.social-icon-footer(src=url_for(theme.socials.youtube), alt='Youtube')
      .text-center.element-footer
        p.info-footer CONTÁCTANOS
        hr
        p grupocomplejidad@gmail.com
        p complejidad@udistrital.edu.co
      .element-footer
        p Pertenecemos a:
        hr
        .text-center
          a(href='https://www.udistrital.edu.co', target='_blank')
            img.img-thumbnail.shield-footer(src=url_for(theme.socials.udistrital), alt='UDistrital')
    .row
      .col.text-center
        hr
        h5.info-footer SUSCRÍBETE
        hr
        p Te enviamos novedades
        button.btn.btn-light(type='button', data-toggle='modal', data-target='#suscribeModal') Enviar
        hr
        div
          p Grupo Complejidad Universidad Distrital
          p
            .copyright
                - var currentDate = new Date();
                - var currentYear = currentDate.getFullYear();
                - var copyrightYear = ! theme.copyright.since || currentYear == theme.copyright.since ? currentYear : theme.copyright.since + ' - ' + currentYear;
                - var copyrightURL = theme.copyright.url || config.url
                - var copyrightName = theme.copyright.name || config.author
                p &copy; #{copyrightYear} #[a(href=copyrightURL) #{copyrightName}]
                    br
                    | Powered by #[a(href='https://hexo.io/', target='_blank') Hexo] &amp; #[a(href=theme.url, target='_blank') #{theme.name}].
            br
            | Bogota D.C, Colombia
  
            
                
